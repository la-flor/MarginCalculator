{
    "name": "Margin Calculator",
    "version": "1.1",
    "description": "Calculate the profit margin for a product by entering the item cost",
    "manifest_version": 2,
    "icons": {"128": "scale_128px.png"},
    "browser_action": {
        "default_icon": {
            "16": "scale_16px.png",
            "24": "scale_24px.png",
            "32": "scale_32px.png"
        },
        "default_title": "Margin Calculator",
        "default_popup": "index.html"
    },
    "permissions": ["storage"]
}